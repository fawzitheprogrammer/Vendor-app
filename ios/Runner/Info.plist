<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Wave Mall Vendor</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Wave Seller</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>LSApplicationQueriesSchemes</key>
    <array>
        <string>http</string>
        <string>https</string>
    </array>
	<key>NSBluetoothPeripheralUsageDescription</key>
    <string>Wave Mall Vendor requires Bluetooth access to connect with nearby Bluetooth devices for a seamless shopping experience, including digital payments and product information sharing.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>Wave Mall Vendor requires continuous location access to provide real-time location-based services such as tracking deliveries and offering location-specific promotions to customers.</string>
    <key>NSBluetoothAlwaysUsageDescription</key>
    <string>Wave Mall Vendor requires always-on Bluetooth access to enable seamless connectivity with Bluetooth devices for features such as mobile payments and inventory management.</string>
    <key>NSCameraUsageDescription</key>
	<string>Wave Mall Vendor needs camera access for taking photo for user profile picture.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Wave Mall Vendor needs access to location to manage delivery address when add a new address.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Wave Mall Vendor needs photo gallery access for taking photo for user profile picture.</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>fetch</string>
		<string>remote-notification</string>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
    	<array>
    		<string>UIInterfaceOrientationPortrait</string>
    		<string>UIInterfaceOrientationLandscapeLeft</string>
    		<string>UIInterfaceOrientationLandscapeRight</string>
    	</array>
    	<key>UISupportedInterfaceOrientations~ipad</key>
    	<array>
    		<string>UIInterfaceOrientationPortrait</string>
    		<string>UIInterfaceOrientationPortraitUpsideDown</string>
    		<string>UIInterfaceOrientationLandscapeLeft</string>
    		<string>UIInterfaceOrientationLandscapeRight</string>
    	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
</dict>
</plist>
